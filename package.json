{
  "name": "react-webpack-boilerplate",
  "version": "0.0.2",
  "description": "Exhaustive template for react webpack",
  "main": "src/index.js",
  "repository": "https://github.com/user/repository.git",
  "author": "Your Name <your@email.com>",
  "keywords": [
    "webpack",
    "react",
    "sass",
    "js",
    "javascript"
  ],
  "license": "MIT",
  "scripts": {
    "start": "cross-env PORT=3041 webpack-dev-server --config=config/webpack.dev.js --hot",
    "start:reload": "cross-env PORT=3041 webpack-dev-server --config=config/webpack.dev.js",
    "start:style": "cross-env PORT=3041 cross-env MAP_STYLE=true webpack-dev-server --config=config/webpack.dev.js --hot",
    "build": "rm -rf dist && NODE_ENV=production webpack --config=config/webpack.prod.js --mode production",
    "analyze": "cross-env ENABLE_ANALYZER=true yarn build",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage",
    "flow:status": "flow status --quiet",
    "lint": "eslint --ext .jsx,.js src/ -c .eslintrc.js",
    "lint:fix": "eslint --ext .jsx,.js src/ -c .eslintrc.js"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "~16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "flow-bin": "^0.126.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
}
